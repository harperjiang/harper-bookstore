            <ruleset>pmd-eclipse</ruleset>
        </rule>
        <rule>
            <name>BigIntegerInstantiation</name>
            <ruleset>pmd-eclipse</ruleset>
        </rule>
        <rule>
            <name>AvoidStringBufferField</name>
            <ruleset>pmd-eclipse</ruleset>
        </rule>
