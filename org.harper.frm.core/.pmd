            <ruleset>pmd-eclipse</ruleset>
        </rule>
        <rule>
            <name>ClassNamingConventions</name>
            <ruleset>pmd-eclipse</ruleset>
        </rule>
        <rule>
            <name>EmptyStaticInitializer</name>
            <ruleset>pmd-eclipse</ruleset>
        </rule>
